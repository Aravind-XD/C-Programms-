#include <stdio.h>

int main()
{
    int dividend,divisor,quotient,remainder ;
    printf("Enter any dividend:");
    scanf("%d",&dividend);
    printf("enter any divisor");
    scanf("%d",&divisor);
    
    quotient =dividend/divisor;
    remainder=dividend%divisor;
    printf("quotient=%d",quotient);
    printf("remainder=%d",remainder);
    return 0;

}
